meta {
  name: Get user by id
  type: http
  seq: 6
}

get {
  url: {{BASE_URL}}/api/v1/users/{{userId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res('success'): eq true
  res('message'): eq Found user Test
  res('data.id'): isString
  res('data.email'): eq testuser@mail.com
  res('data.alias'): eq Test
  res('data.role'): eq prospect
  res('..createdAt'): isDefined
  res('..updatedAt'): isDefined
}

